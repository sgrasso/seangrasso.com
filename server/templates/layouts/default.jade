			script(src='http://html5shim.googlecode.com/svn/trunk/html5.js')
		link(rel='stylesheet', href='/public/css/styles.min.css')
		link(rel='stylesheet', type='text/css', href='http://fonts.googleapis.com/css?family=Open 	+Sans:300,400,600,700,800|Montserrat:400,700')
