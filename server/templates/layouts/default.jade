	body

		.mask
			.bubblingG
				span#bubblingG_1
				span#bubblingG_2
				span#bubblingG_3

		div#page-wrapper
			section#home.parallax(style="background-image: url('http://placehold.it/1920x900');", data-stellar-background-ratio='0.6', data-stellar-vertical-offset='20')
				.parallax-overlay
					.container
						.col-md-12
							.avater(style="background-image: url('http://placehold.it/294x294');background-size: cover;")
							.info
								h1 Sean Grasso
								h2 Developer. Father. Javascript Guru 
								include ../partials/social
							.gotosection
								a(href='#top')
									i.fa.fa-angle-double-down
			div#undefined-sticky-wrapper.sticky-wrapper(style="height: 70px;")
				| !{nav}
			div#top
				
			block content
				
			footer
				.container
					.copyright Â©2016 Sean Grasso All rights reserved.

		script(src='http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.js')
		script(src='/public/js/main.min.js')
		block js
