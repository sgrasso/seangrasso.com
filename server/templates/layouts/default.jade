doctype html
html
	head
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		meta(name='description', content='My personal site')
		meta(name='keywords', content='nodejs, node, javascript, web development, Sean Grasso')
		meta(name='author', content='Sean Grasso')

		title #{pageTitle}
		//- link(rel='shortcut icon', href='img/icons/favicon.ico')
